{
  "name": "moonwave-travel",
  "version": "1.0.0",
  "description": "Moonwave Travel - Smart Travel Planning System",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production npm run type-check && npm run lint && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "performance": "npm run build && npm run start",
    "clean": "rm -rf .next out",
    "vercel-build": "npm run type-check && npm run lint && npm run build",
    "oauth:check": "node -e \"console.log('🔐 OAuth Configuration Check:'); const { validateOAuthConfig } = require('./lib/oauth'); const config = validateOAuthConfig(); console.log(config);\"",
    "oauth:status": "npm run dev -- --inspect"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/google.maps": "^3.55.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
